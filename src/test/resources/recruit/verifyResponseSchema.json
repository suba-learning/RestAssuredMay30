    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "token": {
      "type": "string"
    },
    "expiresAt": {
      "type": "integer"
    },
    "issuedAt": {
      "type": "integer"
    }
